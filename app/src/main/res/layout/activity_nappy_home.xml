<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#DADDD8"
    tools:context=".NappyHome">

    <TextView
        android:id="@+id/textView5"
        android:layout_width="379dp"
        android:layout_height="93dp"
        android:text="Nappy Home"
        android:textColor="#415848 "
        android:textSize="38sp"
        android:textStyle="bold"
        tools:layout_editor_absoluteX="25dp"
        tools:layout_editor_absoluteY="27dp"
        tools:ignore="MissingConstraints" />

    <com.google.android.material.button.MaterialButton
        android:id="@+id/button_first"
        android:layout_width="362dp"
        android:layout_height="230dp"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:backgroundTint="@color/color_5"
        android:text="Start Your Booking"
        android:textSize="34sp"
        android:textStyle="bold"
        app:cornerRadius="10dp"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="25dp"
        tools:layout_editor_absoluteY="136dp" />

    <com.google.android.material.button.MaterialButton
        android:id="@+id/button_second"
        android:layout_width="186dp"
        android:layout_height="107dp"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:backgroundTint="@color/color_5"
        android:text="How to Book"
        app:cornerRadius="10dp"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="385dp" />

    <com.google.android.material.button.MaterialButton
        android:id="@+id/bookingHistoryLabel"
        android:layout_width="186dp"
        android:layout_height="107dp"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:backgroundTint="@color/color_5"
        android:text="Booking History"
        app:cornerRadius="10dp"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="209dp"
        tools:layout_editor_absoluteY="385dp" />

    <com.google.android.material.button.MaterialButton
        android:id="@+id/update_Profile"
        android:layout_width="186dp"
        android:layout_height="107dp"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:backgroundTint="@color/color_5"
        android:text="Update Profile"
        app:cornerRadius="10dp"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="533dp" />

    <com.google.android.material.button.MaterialButton
        android:id="@+id/signup"
        android:layout_width="186dp"
        android:layout_height="107dp"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:backgroundTint="@color/color_5"
        android:text="FAQ"
        app:cornerRadius="10dp"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="209dp"
        tools:layout_editor_absoluteY="533dp" />


</androidx.constraintlayout.widget.ConstraintLayout>